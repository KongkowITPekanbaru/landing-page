{
    "version": 2,
    "name": "kongkowitpku",
    "scope": "kongkowitpku",
    "builds": [
      {
        "src": "package.json",
        "use": "@now/static-build",
        "config": { "distDir": "build" }
      }
    ],
    "routes": [
      { "src": "/event-terbaru", "dest": "/event-terbaru.html" },
      { "src": "/lowongan-kerja", "dest": "/lowongan-kerja.html" },
      { "src": "/member", "dest": "/member.html" }
    ]
  }